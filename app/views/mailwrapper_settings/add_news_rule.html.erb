<h2><%= l(:label_mailwrapper_news_rule_new) %></h2>

<% labelled_tabular_form_for :mailwrapper_news_rule, @news_rule, :url => {:action => 'add_news_rule', :project_id => @project} do |f| %>
  <%= error_messages_for 'mailwrapper_news_rule' %>
