<h2><%= l(:label_mailwrapper_news_rule_new) %></h2>

<% labelled_form_for :mailwrapper_news_rule, @mailwrapper_news_rule, :url => {:action => 'add', :project_id => @project} do |f| %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <%= submit_tag l(:button_create) %>
  <%= link_to l(:button_cancel), :controller => 'projects', :action => 'settings', :id => @project.id, :tab => 'mailwrapper' %>
<% end %>

